{
  "consumer": {
    "name": "ProductsUI"
  },
  "interactions": [
    {
      "description": "a request for a product with id 12345",
      "providerStates": [
        {
          "name": "there exists a product with id 12345"
        }
      ],
      "request": {
        "headers": {
          "Accept": "application/json"
        },
        "method": "GET",
        "path": "/products/12345"
      },
      "response": {
        "body": {
          "id": 1,
          "name": "Wireless Mouse"
        },
        "headers": {
          "Content-Type": "application/json"
        },
        "matchingRules": {
          "body": {
            "$": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "type"
                }
              ]
            }
          },
          "header": {}
        },
        "status": 200
      }
    },
    {
      "description": "a request for all products",
      "providerStates": [
        {
          "name": "there is a non-empty list of products"
        }
      ],
      "request": {
        "headers": {
          "Accept": "application/json"
        },
        "method": "GET",
        "path": "/products"
      },
      "response": {
        "body": [
          {
            "id": 1,
            "name": "Wireless Mouse"
          }
        ],
        "headers": {
          "Content-Type": "application/json"
        },
        "matchingRules": {
          "body": {
            "$": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "type",
                  "min": 1
                }
              ]
            }
          },
          "header": {}
        },
        "status": 200
      }
    }
  ],
  "metadata": {
    "pact-js": {
      "version": "12.1.0"
    },
    "pactRust": {
      "ffi": "0.4.7",
      "models": "1.1.9"
    },
    "pactSpecification": {
      "version": "3.0.0"
    }
  },
  "provider": {
    "name": "ProductsAPI"
  }
}